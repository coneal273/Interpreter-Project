{
  "compilerOptions": {
    "jsx": "react",
    "module": "es2015",
    "noImplicitAny": false,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/index.tsx"],
  "exclude": ["node_modules", "build"]
}
